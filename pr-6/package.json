{
  "name": "pr-6",
  "version": "1.0.0",
  "description": "Student Management API with Authentication, Authorization, Testing, Logging, Monitoring and Swagger",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "create-db": "node scripts/create-db.js",
    "migrate": "node migrations/migrate.js",
    "setup": "npm run create-db && npm run migrate",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-status-monitor": "^1.2.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/migrations/**",
      "!**/scripts/**",
      "!index.js"
    ]
  }
}
